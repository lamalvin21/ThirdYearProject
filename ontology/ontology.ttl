#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/polifonia/ontology/organs/archive
oont:archive rdf:type owl:ObjectProperty ;
             rdfs:domain oont:ArchiveInstrument ;
             rdfs:comment "BIGINT UNSIGNED NOT NULL" .


###  http://w3id.org/polifonia/ontology/organs/builder
oont:builder rdf:type owl:ObjectProperty ;
             rdfs:domain oont:Disposition ;
             rdfs:range oont:Builder ;
             rdfs:comment "BIGINT UNSIGNED NULL DEFAULT NULL" .


###  http://w3id.org/polifonia/ontology/organs/country
oont:country rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( oont:Address
                                         oont:Builder
                                         oont:Instrument
                                       )
                         ] .


###  http://w3id.org/polifonia/ontology/organs/coupler
oont:coupler rdf:type owl:ObjectProperty ;
             rdfs:domain oont:AdditionalSpecification ;
             rdfs:range oont:Coupler ;
             rdfs:comment "BIGINT UNSIGNED NULL DEFAULT NULL" .


###  http://w3id.org/polifonia/ontology/organs/disposition
oont:disposition rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( oont:AdditionalSpecification
                                             oont:Manual
                                           )
                             ] ;
                 rdfs:comment "BIGINT UNSIGNED NOT NULL" .


###  http://w3id.org/polifonia/ontology/organs/fileable
oont:fileable rdf:type owl:ObjectProperty ;
              rdfs:domain oont:File ;
              rdfs:comment "BIGINT UNSIGNED NOT NULL" .


###  http://w3id.org/polifonia/ontology/organs/instrument
oont:instrument rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( oont:Address
                                            oont:ArchiveInstrument
                                            oont:Disposition
                                          )
                            ] ;
                rdfs:range oont:Instrument ;
                rdfs:comment "BIGINT UNSIGNED NOT NULL" ,
                             "BIGINT UNSIGNED NULL DEFAULT NULL" .


###  http://w3id.org/polifonia/ontology/organs/locationImage
oont:locationImage rdf:type owl:ObjectProperty ;
                   rdfs:domain oont:Instrument ;
                   rdfs:comment "BIGINT UNSIGNED NULL DEFAULT NULL" .


###  http://w3id.org/polifonia/ontology/organs/manual
oont:manual rdf:type owl:ObjectProperty ;
            rdfs:domain oont:Stop ;
            rdfs:comment "BIGINT UNSIGNED NOT NULL" .


###  http://w3id.org/polifonia/ontology/organs/mentor
oont:mentor rdf:type owl:ObjectProperty ;
            rdfs:domain oont:Instrument ;
            rdfs:comment "BIGINT UNSIGNED NULL DEFAULT NULL" .


###  http://w3id.org/polifonia/ontology/organs/organization
oont:organization rdf:type owl:ObjectProperty ;
                  rdfs:domain oont:Address ,
                              oont:Archive ,
                              oont:Instrument ;
                  rdfs:comment "BIGINT UNSIGNED NULL DEFAULT NULL" .


###  http://w3id.org/polifonia/ontology/organs/owner
oont:owner rdf:type owl:ObjectProperty ;
           rdfs:domain oont:Archive ,
                       oont:Instrument ;
           rdfs:comment "BIGINT UNSIGNED NULL DEFAULT NULL" .


###  http://w3id.org/polifonia/ontology/organs/person
oont:person rdf:type owl:ObjectProperty ;
            rdfs:domain oont:Address ;
            rdfs:comment "BIGINT UNSIGNED NULL DEFAULT NULL" .


